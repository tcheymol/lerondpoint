{% extends 'base.html.twig' %}

{% block title %}{{ block('title') }}{% endblock %}
{% block bodybg %}{{ block('bodybg') }}{% endblock %}

{% block content %}
    <h1 class="col-12 text-center text-dark mt-3">{{ block('title') }}</h1>
    <div class="col-12 col-sm-10 offset-sm-1 col-md-8 offset-md-2 col-lg-6 offset-lg-3">
        <div class="card p-5 m-3">
            {% block cardContent %}{% endblock %}
        </div>
        {% block outterContent %}{% endblock %}
    </div>
{% endblock %}
