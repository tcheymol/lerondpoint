{% set messages = [] %}

{% for child in form.children|filter(c => c.vars.errors is defined) %}
    {% for error in child.vars.errors %}
        {% set messages = messages|merge([error.message]) %}
    {% endfor %}
{% endfor %}
{% if messages|length %}
    {% for message in messages %}
        <div class="alert alert-danger m-5">
            {{ message|trans }}
        </div>
    {% endfor %}
{% endif %}
